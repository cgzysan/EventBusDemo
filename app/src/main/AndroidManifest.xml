<?xml version="1.0" encoding="utf-8"?>
<manifest package="ysan.eventbusdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.BroadcastActivity"/>
        <activity android:name=".activity.EventBusActivity"/>
        <service android:name=".service.BroadService"/>
        <service android:name=".service.EventbusService"/>
        <service android:name=".service.NewsService"/>
    </application>

</manifest>